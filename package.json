{
    "name": "airline-booking-api",
    "version": "1.0.0",
    "description": "my server",
    "main": "dist/server.js",
    "author": "duyplk",
    "license": "MIT",
    "scripts": {
        "gen:key": "rm -rf src/messages/MessageKeys.ts && ts-node src/messages/GenMessage.ts && prettier --write src/messages/MessageKeys.ts",
        "clean": "rm -rf dist && mkdir dist",
        "build": "yarn run clean && tsc && tsc-alias",
        "start": "node dist/server.js",
        "dev": "nodemon src/server.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "docker:db:down": "docker-compose -f src/docker/postgresql.yml down -v",
        "docker:db:up": "docker-compose -f src/docker/postgresql.yml up -d",
        "docker:rd:down": "docker-compose -f src/docker/redis.yml down -v",
        "docker:rd:up": "docker-compose -f src/docker/redis.yml up -d"
    },
    "dependencies": {
        "argon2": "^0.31.1",
        "axios": "^1.6.2",
        "cloudinary": "^1.41.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "i18n": "^0.15.1",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.7",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "qs": "^6.11.2",
        "redis": "^4.6.10",
        "typeorm": "^0.3.17",
        "uuid": "^9.0.1",
        "validator": "^13.11.0",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.17",
        "@types/express-serve-static-core": "^4.17.37",
        "@types/i18n": "^0.13.6",
        "@types/jsonwebtoken": "^9.0.3",
        "@types/morgan": "^1.9.5",
        "@types/multer": "^1.4.9",
        "@types/node": "^20.6.0",
        "@types/nodemailer": "^6.4.13",
        "@types/uuid": "^9.0.6",
        "@types/validator": "^13.11.2",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.7",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    }
}
